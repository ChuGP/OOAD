package ClassDetailInfo;

public class ClassDetailInfo {
	private String _class_name;
	private ClassRelarionship _ClassRelarionship;
	private MemberVariable _MemberVariable;
	private MemerFunction _MemberFunction;
	public ClassDetailInfo() {}
	public ClassDetailInfo(String class_name, ClassRelarionship ClassRelarionship, MemberVariable MemberVariable,MemerFunction _MemberFunction) {
		_class_name = class_name;
		
	}
    
	// Setter
	public void _class_name_setter(String class_name)
	{
		_class_name =  class_name;
		
	}
	
	public void _reference_setter(String reference)
	{
		_reference =  reference;
		
	}
	
	public void _variable_type_setter(String variable_type)
	{
		_variable_type =  variable_type;
		
	}
	
	public void _variable_name_setter(String variable_name)
	{
		_variable_name =  variable_name;
		
	}
	
	// Getter
	public String _class_name__getter()
	{
		return _class_name;
		
	}
	
	public String reference_getter()
	{
		return _reference;
		
	}
	
	public String variable_type__getter()
	{
		return _variable_type;
		
	}
	
	public String _variable_name__getter()
	{
		return _variable_name;
		
	}
	
	
}
