package uml_generator;

public class class_Detail {
	private String _reference,_variable_type, _variable_name;
	public class_Detail(String reference,String variable_type,String variable_name) {
		_reference = reference;
		_variable_type = variable_type;
		_variable_name = variable_name;
	}
    
	public String reference_getter()
	{
		return _reference;
		
	}
	
	public String variable_type__getter()
	{
		return _variable_type;
		
	}
	
	public String _variable_name__getter()
	{
		return _variable_name;
		
	}
}
